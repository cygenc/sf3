{
    "name": "cygenc/test-symfony",
    "description": "Project to test Symfony",
    "version": "1.0.0-dev",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.0.8",
        "symfony/cache": "5.0.8",
        "symfony/console": "5.0.8",
        "symfony/flex": "1.7.1",
        "symfony/form": "5.0.8",
        "symfony/monolog-bundle": "3.5.0",
        "symfony/polyfill-uuid": "1.17.0",
        "symfony/security-bundle": "5.0.8",
        "symfony/security-csrf": "5.0.8",
        "symfony/stopwatch": "5.0.8",
        "symfony/translation": "5.0.8",
        "symfony/twig-bridge": "5.0.8",
        "symfony/twig-bundle": "5.0.8",
        "symfony/validator": "5.0.8",
        "symfony/web-profiler-bundle": "5.0.8",
        "symfony/webpack-encore-bundle": "1.7.3",
        "symfony/yaml": "5.0.8",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "symfony/debug-pack": "1.0.8",
        "symfony/dotenv": "5.0.8",
        "symfony/maker-bundle": "1.19.0",
        "symfony/phpunit-bridge": "5.0.8",
        "symfony/var-dumper": "5.2.1",
        "symfony/web-server-bundle": "4.4.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C38A85YR0FE60NSWG3HTGFNG",
            "allow-contrib": false,
            "require": "^5.0"
        }
    }
}
